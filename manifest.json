{
    "manifest_version": 3,
    "name": "Page Search",
    "version": "1.0",
    "author": "John Lazaroo",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "host_permissions": ["<all_urls>", "activeTab"],
    "action": {
        "default_popup": "index.html",
        "default_title": "Page Search"
    }
}